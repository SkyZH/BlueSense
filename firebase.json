{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/feed", "function": "feed"
      }, {
        "source": "/thumbnail/**", "function": "report"
      }, {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
